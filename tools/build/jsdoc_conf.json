{
    "plugins": [
        "node_modules/better-docs/category"
    ],
    "recurseDepth": 1,
    "source": {
		"include": [ "output/DuESF.jsxinc" ],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "copyright"             : "Copyright (c) 2017-2022 Nicolas Dufresne and contributors",
        "includeDate"           : true,
        "search"                : "true",
        "better-docs": {
            "name": "Duik API Reference",
            "logo": "",
            "title": "",
            "css": "jsdoc.css",
            "hideGenerator": false,
            "navLinks": [
                {
                    "label": "Website",
                    "href": "https://rxlaboratory.org"
                },
                {
                    "label": "Source code",
                    "href": "https://github.com/RxLaboratory/DuESF"
                },
                {
                    "label": "Donate",
                    "href": "http://donate.rxlab.info"
                }
            ]
        }
    },
    "linenums": true,
    "meta": {
        "title": "DuESF",
        "description": "The Duduf ExtendScript Framework reference",
        "keyword": "ExtendScript, Adobe"
    },
	"opts": {
        "template": "node_modules/better-docs",
		"recurse": false,
        "destination": "../../docs"
    }
}