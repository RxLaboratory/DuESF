{
    "plugins": [],
    "recurseDepth": 1,
    "source": {
		"include": [ "output/DuESF.jsxinc" ],
        "includePattern": ".+\\.jsxinc$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "copyright"             : "Copyright (c) 2017-2021 Nicolas Dufresne and contributors",
        "includeDate"           : true,
        "search"                : "true"
    },
    "linenums": true,
    "meta": {
        "title": "DuESF",
        "description": "The Duduf ExtendScript Framework reference",
        "keyword": "ExtendScript, Adobe"
    },
	"opts": {
        "template": "node_modules/jaguarjs-jsdoc",
		"recurse": false,
        "destination": "../../docs"
    }
}